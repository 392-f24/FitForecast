    var optionValue = jQuery(this).val();
    if($('.sizing-chart').length) {
      $('.sizing-chart li[data-color="'+ optionValue +'"]').show();
      $('.sizing-chart li:not([data-color="'+ optionValue +'"])').hide();
    }
    jQuery(this)
      .closest('form')
      .find('.single-option-selector')
      .eq(optionIndex)
      .val(optionValue)
      .trigger('change');
  });
  if(jQuery(".selector-wrapper label").length) {
